<h1>Jared Gebel</h1>
<h1>Sample Projects</h1>

{{#each projects as |project|}}
  <div class="project">
    <div class="project-name">
      <a href="/projects/{{project.name}}">
        {{project.name}}
      </a>
    </div>


    <div class="images">
      {{#each project.imgSrc as |imgsrc|}}
        <div class="img">
          <img src="{{imgsrc}}">
        </div>
      {{/each}}
    </div>
  </div>
{{/each}}
