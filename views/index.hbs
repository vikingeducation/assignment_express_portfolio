<html>
<head>
</head>
<body>
	<h1>Projects</h1>

	{{#each projects as |project|}}
		<div class="project">
	    <div class="project-name">
	      <a href="/projects/{{project.name}}">
	         {{project.name}}
	      </a>
	    </div>

	    <div class="project-language">
        {{project.language}}
	    </div>

	    <div class="project-github">
        <a href="{{project.github}}">
        	Github Repository
        </a>
	    </div>
	  </div>
	{{/each}}
</body>
</html>